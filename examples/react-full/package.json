    "express": "^4.17.1",
    "history-server": "^1.3.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "serve": "^12.0.1",
    "ts-node": "^10.3.0",
